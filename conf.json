{
    "tags": {
        "allowUnknownTags": true
    },
    "markdown": {
        "parser": "evilstreak"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false
        },
        "linenums": false
    },
    "source": {
        "include": ["dist/ct-jquery-fancySelect.js"]
    },
    "opts": {
        "template": "node_modules/jaguarjs-jsdoc",
        "encoding": "utf8",
        "recurse": true,
        "private": true,
        "lenient": true,
        "destination": "documentation/js-docs/"
    }
}